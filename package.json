{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --forceExit",
    "lint": "eslint **/*.js --ignore-pattern node_modules/",
    "lint:fix": "eslint **/*.js --fix",
    "e2e": "detox test --configuration ios.sim"
  },
  "detox": {
    "configurations": {
      "ios.sim": {
        "binaryPath": "bin/Exponent.app",
        "type": "ios.simulator",
        "name": "iPhone 11 Pro Max"
      }
    },
    "test-runner": "jest"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>setupTests.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|unimodules-permissions-interface|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|sentry-expo|native-base)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js",
      "!**/web-build/register-service-worker.js",
      "!react-native.config.js",
      "!**/e2e/init.js",
      "!**/assets/floormaps/hall/*.js",
      "!**/services/getApiKey.js",
      "!**/config.js",
      "!**/App.js",
      "!**/database.config.js",
      "!**/routes/routeStack.js",
      "!**/constants/buildingCoordinates.js",
      "!**/screens/Directions.js",
      "!**/screens/PreviewDirections.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/web-build/register-service-worker.js"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "@mapbox/polyline": "^1.1.0",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/masked-view": "0.1.6",
    "@react-navigation/drawer": "^5.3.2",
    "@react-navigation/native": "^5.1.1",
    "@react-navigation/stack": "^5.2.3",
    "acorn": "^7.1.0",
    "detox-expo-helpers": "^0.6.0",
    "expo": "^37.0.0",
    "expo-detox-hook": "^1.0.10",
    "expo-font": "~8.1.0",
    "expo-location": "~8.1.0",
    "firebase": "7.9.0",
    "galio-framework": "^0.6.3",
    "geolib": "^3.2.1",
    "jest": "^25.1.0",
    "native-base": "^2.13.8",
    "popup-ui": "^1.0.10",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.0.tar.gz",
    "react-native-elements": "^1.2.7",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-maps": "0.26.1",
    "react-native-modal": "^11.5.4",
    "react-native-paper": "^3.5.1",
    "react-native-reanimated": "~1.7.0",
    "react-native-render-html": "^4.2.0",
    "react-native-safe-area-context": "0.7.3",
    "react-native-search-header": "^0.3.5",
    "react-native-searchable-dropdown": "^1.1.1",
    "react-native-svg": "11.0.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-switch": "^1.5.0",
    "react-native-switch-selector": "^2.0.3",
    "react-native-web": "^0.11.7",
    "react-native-webview": "8.1.1",
    "react-navigation": "^4.0.10",
    "react-navigation-drawer": "^2.4.6",
    "react-navigation-stack": "^2.0.15",
    "react-navigation-transitions": "^1.0.12",
    "react-native-screens": "~2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@iconify/icons-fa-solid": "^1.0.7",
    "@iconify/react": "^1.1.2",
    "babel-preset-expo": "^8.1.0",
    "detox": "^16.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "jest-enzyme": "^7.1.2",
    "jest-expo": "^37.0.0",
    "prettier": "1.19.1",
    "react-native-dotenv": "^0.2.0",
    "react-native-svg-asset-plugin": "^0.3.0",
    "react-test-renderer": "^16.12.0"
  },
  "private": true
}
